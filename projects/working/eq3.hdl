/** If the three given bits are equal, sets out to 1; else sets out to 0 */
CHIP eq3 {
    IN a, b, c;
    OUT out;
    PARTS:
      Xor(a=a, b=b, out=neq1);    // Xor(a,b) -> neq1
      Xor(a=b, b=c, out=neq2);         // Xor(b,c)->neq2
      Or (a=neq1, b=neq2, out=outOr);
      Not(in=outOr, out=out);
}
